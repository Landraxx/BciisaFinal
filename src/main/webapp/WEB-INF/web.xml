<?xml version="1.0" encoding="UTF-8"?>
<web-app version="4.0" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd">
    <servlet>
        <servlet-name>NewServlet</servlet-name>
        <servlet-class>Controller.NewServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NewServletLogin</servlet-name>
        <servlet-class>Controller.NewServletLogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>registroCliente</servlet-name>
        <servlet-class>Controller.registroCliente</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CatalogoL</servlet-name>
        <servlet-class>Controller.CatalogoL</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>Controller.Login</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>NewServlet</servlet-name>
        <url-pattern>/NewServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NewServletLogin</servlet-name>
        <url-pattern>/NewServletLogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>registroCliente</servlet-name>
        <url-pattern>/registroCliente</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CatalogoL</servlet-name>
        <url-pattern>/CatalogoL</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
